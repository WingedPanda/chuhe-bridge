<section id="content">
    <!--start container-->
    <div class="chuhe-dashboard-container">
        <div class='chuhe-chart-container'>
            <div id='chuhe-bridge-scene' class='chuhe-3d-item'>

            </div>
            <div id='map' class='chuhe-map-item'>
            </div>
        </div>
        <div class="chuhe-card-container">
            <div class="chuhe-card-item">
              <!-- 挠度传感器-->
              <%= deflectionSensor %>
            </div>
            <div class="chuhe-card-item">
              <!-- 位移传感器-->
              <%= shiftSensor %>
            </div>
            <div class="chuhe-card-item">
              <!-- 垂直度传感器-->
              <%= perpendicularitySensor %>
            </div>
            <div class="chuhe-card-item">
              <!-- 应变传感器-->
              <%= strainSensor %>
            </div>
            <div class="chuhe-card-item">
              <!-- 索利传感器-->
              <%= cableSensor %>
            </div>
            <div class="chuhe-card-item">
              <!-- 震动传感器-->
              <%= vibrationSensor %>
            </div>
            <div class="chuhe-card-item">
              <!-- 荷载传感器-->
              <%= loadSensor %>
            </div>
            <div class="chuhe-card-item">
              <!-- 服饰度传感器-->
              <%= corrosionSensor %>
            </div>
        </div>
    </div>
    <!--end container-->
</section>
